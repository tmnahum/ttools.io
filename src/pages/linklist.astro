    <style>
        html {
            font-size: x-large;
            color-scheme: light dark;
            /* ^ This makes browsers switch the default color scheme from white to black background if the user prefers dark mode */
            font-family: Arial, Helvetica, sans-serif;
            box-sizing: border-box;
        }

        body {
            padding: .4rem;
            padding-bottom: 0;
            max-width: 600px;
            margin: 0 auto;
        }

        h2 {
            margin-top: .5rem;
        }

        p,
        ul {
            margin-top: 0.5rem;
            margin-bottom: 0.5em;
        }

        hr {
            margin-top: 1rem;
            margin-bottom: 1rem;
        }

        li {
            margin-top: 0.25rem;
        }

        /* ------------- */
        li a {
            display: inline-block;
            /* margin: 1px; */
        }


        /* this makes stuff happen when you hover! */
        li a:hover {
            /* transform: rotate(-5deg); */
            transform: scaleY(1.2) scaleX(1.05);
            filter: hue-rotate(90deg);
            transition: all 0.5s;
            animation: link_hover 5s linear infinite;
        }

        a:hover {
            display: inline-block;
            transform: scaleY(1.05) scaleX(1.05);
            transition: all 0.1s;
            /* animation: link_hover 5s linear infinite; */
        }

        span>a:hover {
            /* buttons */
            transform: scaleY(1.5) scaleX(1.05);
            animation: link_hover 4s linear infinite;

            transition: all 1.5s;
        }

        span>a {

            transition: all 1s;
        }

        @keyframes link_hover {
            from {
                filter: hue-rotate(0deg);
            }

            to {
                /* transform: scaleY(1.2) scaleX(1.05); */
                filter: hue-rotate(360deg);
            }
        }

        .unstyled {
            text-decoration: none;
            /* text: white; */
            color: white
        }
    </style>

</head>

